<% if current_user %>
  Welcome, <%= current_user.email %>
  <%= link_to "Logout", logout_path, method: :delete %>
  <%= link_to "Your Links", links_path, method: :get %>
<% else %>
  <%= link_to "Log in", login_path, method: :get %>
  <%= link_to "Sign Up", new_user_path, method: :get %>
<% end %>
